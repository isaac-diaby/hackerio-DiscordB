{
  "name": "hackerio-discord",
  "version": "1.4.0",
  "engines": {
    "node": "14.x"
  },
  "description": "Hacker.io is a discord bot that simulates a hacking online game",
  "main": "src/index.ts",
  "scripts": {
    "start": "ts-node src/discordBot.ts & ts-node src/server.ts",
    "start-bot": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec ts-node src/discordBot.ts",
    "start-server": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec ts-node src/server.ts",
    "build": "tsc -p ."
  },
  "dependencies": {
    "blapi": "^2.0.0",
    "discord.js": "^12.2.0",
    "dotenv": "^8.2.0",
    "express": "4.17.1",
    "express-handlebars": "^4.0.4",
    "mongoose": "^5.9.18",
    "request": "^2.88.0",
    "stripe": "^8.x"
  },
  "devDependencies": {
    "@types/dotenv": "6.1.1",
    "@types/express": "4.17.0",
    "@types/mongoose": "^5.3.26",
    "@types/express-handlebars": "^3.1.0",
    "@types/node": "^11.13.5",
    "@types/node-fetch": "^2.5.7",
    "@types/ws": "^7.2.5",
    "nodemon": "^1.18.4",
    "ts-node": "^8.3.0",
    "tslint": "^5.16.0",
    "typescript": "^3.5.3"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/isaac-diaby/hackerio-DiscordB.git"
  },
  "keywords": [
    "hackerio",
    "discord",
    "bot",
    "online",
    "game"
  ],
  "private": true,
  "author": "isaac-diaby <me@isaacdiaby.tech>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/isaac-diaby/hackerio-DiscordB/issues"
  },
  "homepage": "https://github.com/isaac-diaby/hackerio-DiscordB#readme"
}
